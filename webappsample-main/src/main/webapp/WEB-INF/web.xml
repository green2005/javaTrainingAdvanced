    <servlet>
        <servlet-name>deps</servlet-name>
        <servlet-class>by.grodno.pvt.site.webappsample.DepsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>deps</servlet-name>
        <url-pattern>/deps</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>depsdelete</servlet-name>
        <servlet-class>by.grodno.pvt.site.webappsample.DepsDeleteServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>depsdelete</servlet-name>
        <url-pattern>/deps/delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>depsedit</servlet-name>
        <servlet-class>by.grodno.pvt.site.webappsample.DepsEditServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>depsedit</servlet-name>
        <url-pattern>/deps/edit</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>by.grodno.pvt.site.webappsample.HomePageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jstl1</servlet-name>
        <servlet-class>by.grodno.pvt.site.webappsample.JstlServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jstl2</servlet-name>
        <servlet-class>by.grodno.pvt.site.webappsample.JstlServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jstl3</servlet-name>
        <servlet-class>by.grodno.pvt.site.webappsample.JstlServlet3</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>EditServlet</servlet-name>
        <servlet-class>by.grodno.pvt.site.webappsample.UserEditServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>EditServlet</servlet-name>
        <url-pattern>/user/edit</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>Jstl1</servlet-name>
        <url-pattern>/jstl1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Jstl2</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Jstl3</servlet-name>
        <url-pattern>/user/delete</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
</web-app>
